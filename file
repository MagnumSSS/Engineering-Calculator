#ПередаточноЧисло-i0
Zg / Zp

#СкоростьКорпуса-wk
wp / i0

#ЧислоЗубьевПолуоси-Zc
2 * Zs

#ДелительнДиаметрСателлита-ds
m_diff * Zs

#ДелительнДиаметрПолуоси-dc
m_diff * Zc

#КонуснРасстДифференциала-Re_diff
m_diff * Zc / 1.41421356

#ДелительнДиаметрВедущей-dp
m_main * Zp

#ДелительнДиаметрВедомой-dg
m_main * Zg

#МежосевоеГлавной-A_main
m_main * ( Zp + Zg ) / 1.41421356

#СкоростьЛевойПолуоси-w1
2 * wk - w2

#СкоростьПравойПолуоси-w2
2 * wk - w1

#МинЗубьевСателлита-Zmin
17 * 0.70710678

#ПроверкаСборки-check
Zc - 2 * Zs

